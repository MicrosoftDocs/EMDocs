{
  "build": {
    "content": [
      {
        "files": [
          "**/**.md",
          "**/**.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "**/_themes.pdf/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.svg",
          "**/*.jpg",
          "**/*.json"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "**/docfx.json"
        ]
      }
    ],
    "dest": "microsoft-365-enterprise",
    "template": "docs.html",
    "globalMetadata": {
      "layout": "Conceptual",
      "breadcrumb_path": "/microsoft-365-enterprise/bread/toc.json"
    },
    "markdownEngineName": "markdig"
  }
}
